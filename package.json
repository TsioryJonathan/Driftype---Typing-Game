{
  "name": "typing-website-template",
  "version": "1.0.0",
  "description": "A barebones typing website for you to complete. ",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "server": "cd backend && npm i --omit=dev && node ./src/index.js",
    "start": "npm run build && npm run server"
  },
  "keywords": [
    "typing",
    "game",
    "speedtyping",
    "toja",
    "driftype"
  ],
  "author": "HEI Madagascar : Toky & Fiantso",
  "license": "ISC",
  "dependencies": {
    "@barba/core": "^2.10.3",
    "@tailwindcss/cli": "^4.1.1",
    "animate.css": "^4.1.1",
    "echarts": "^5.6.0",
    "tailwindcss": "^4.1.1",
    "tinykeys": "^3.0.0",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
